  "ScriptText": "var sampleHtml = '<p class=\"lyrics\"><span style=\"font-size: 12pt;\">Sample Text Line 1</span></p><p class=\"lyrics\"><b>Sample Text Line 2</b></p>';\r\nvar TXT_HTML = Matrix.getObject(\"TXT_HTML\");\r\nvar webEditor = null;\r\nvar articleViewer = null;\r\nTXT_HTML.Text = \"\";\r\n/**************************************\r\n * 문서 로드 된 후 AutoRefresh 수행 전에 발생합니다.\r\n * * arguments :\r\n **************************************/\r\nvar OnLoadComplete = function (_sender, _args) {\r\n    var getValue = Matrix.getObject(\"ADD_EDITOR\");\r\n    webEditor = getValue.getScriptClass(\"SmartEditor\");\r\n    webEditor.SetValue(sampleHtml);\r\n    var setValue = Matrix.getObject(\"ADD_VIEWER\");\r\n    articleViewer = setValue.getScriptClass(\"SmartEditor\");\r\n    articleViewer.SetViewerMode();\r\n    articleViewer.SetValue('');\r\n};\r\n/*****************************************\r\n * 버튼 컨트롤이 클릭되는 시점에 발생합니다.\r\n * * arguments :\r\n *\t\t string\tId (Readonly:False) : 컨트롤이름\r\n *\t\t string\tText (Readonly:False) : 라벨 값\r\n *****************************************/\r\nvar OnButtonClick = function (_sender, args) {\r\n    if (args.Id == \"BTN_GET_VAL\") {\r\n        TXT_HTML.Text = webEditor.GetValue();\r\n    }\r\n    else if (args.Id == \"BTN_SET_VAL\") {\r\n        articleViewer.SetValue(TXT_HTML.Text);\r\n    }\r\n};\r\n",
