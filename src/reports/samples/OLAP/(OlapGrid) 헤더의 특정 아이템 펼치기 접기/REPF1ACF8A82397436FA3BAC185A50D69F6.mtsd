  "ScriptText": "/*****************************\r\n * i-AUD Client Sample\r\n *****************************/\r\nvar btnExpandCollapsed = Matrix.getObject(\"btnExpandCollapsed\");\r\nvar tbxItems = Matrix.getObject(\"tbxItems\");\r\nvar rdoExpand = Matrix.getObject(\"rdoExpand\");\r\nvar rdoCollapsed = Matrix.getObject(\"rdoCollapsed\");\r\nvar OlapGrid = Matrix.getObject(\"OlapGrid\");\r\nvar cboFields = Matrix.getObject(\"cboFields\");\r\n/*****************************************\r\n* 전체 확장/축소 실행하기\r\n*****************************************/\r\nbtnExpandCollapsed.OnClick = function (sender, args) {\r\n    if (!rdoExpand.IsChecked) {\r\n        OlapGrid.ExpandAll();\r\n    }\r\n    else {\r\n        OlapGrid.CollapsedAll();\r\n    }\r\n};\r\n/*****************************************\r\n* 컨트롤에 데이터셋이 바인딩된 후 발생합니다.\r\n* * arguments :\r\n*\t\t string\tId (Readonly:False) : 컨트롤이름\r\n*\t\t int\tRecordCount (Readonly:False) : 데이터셋의 레코드 수량\r\n*****************************************/\r\nOlapGrid.OnDataBindEnd = function (sender, args) {\r\n    OlapGrid.Expand(cboFields.Value, tbxItems.Text, rdoExpand.IsChecked);\r\n    OlapGrid.Calculate();\r\n};\r\n/**\r\n * Olap의 디멘젼 필드 목록을 추출하여 ComboBox에 출력 합니다.\r\n */\r\nMatrix.OnDocumentLoadComplete = function () {\r\n    var ds = Matrix.CreateDataSet();\r\n    var dt = ds.CreateTable(\"DATA\");\r\n    dt.AddColumn(\"NAME\", false);\r\n    dt.AddColumn(\"DESCRIPTION\", false);\r\n    var fields = OlapGrid.getFields();\r\n    for (var i = 0; i < fields.length; i++) {\r\n        if (fields[i].Category != 2) { /*Measure가 아닌 것들...*/\r\n            dt.AppendRow([\r\n                fields[i].Name,\r\n                fields[i].Caption\r\n            ]);\r\n        }\r\n    }\r\n    cboFields.SetDataSet(ds);\r\n};\r\n",
