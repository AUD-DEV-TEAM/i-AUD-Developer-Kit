      "MODULE_DESCRIPTION": "OlapGrid - MetaTreeView 배치개인화 모듈",
      "SCRIPT_TEXT": "/**\n * \n * @param olap 대상 OlapGrid\n * @param view 대상 OlapTreeView\n * @param imgButton 배치개인화 Save/Load Image\n * @param enableChkBox 배치개인화 사용여부 결정 CheckBox\n */\nvar UserDefinedLayoutManager = function(olap, view, imgButton, enableChkBox){\n\t\n\tthis.OlapGrid = Matrix.getObject(olap);\n\tthis.TreeView = Matrix.getObject(view);\n\t/** 배치개인화 Save/Load하는 Image */\n\tthis.Button   = Matrix.getObject(imgButton);\n\tthis.EnableLayout = enableChkBox ? Matrix.getObject(enableChkBox) : null;\n\tthis.LayoutReportName = \"TEMPLATE_SD_USER_DEFINED_LAYOUT\";\n\tthis.USE_INIT_LAYOUT = false;\n\t\n\t/*배치 개인화 사용이 활성화 된 경우만 처리 합니다.*/\n\tthis.IsEnable = function(){\n        if(SCRIPT.isViewerMode === false){\n\t\t\treturn false;\n\t\t}\n\t\tif(!this.EnableLayout.Checked){\n\t\t\treturn false;\n\t\t}\n\t\tvar reportInfo = Matrix.GetReportInfo();\n\t\tif(!reportInfo || !reportInfo.CODE){\n\t\t\t//보고서 정보가 없으면 Disable\n\t\t\treturn false;\n\t\t}\n\t\treturn true;\n\t}\n\tthis.init = function(){\n\t\tvar _this =this;\n\t\tthis.Button.OnClick = function(s, e){\n\t\t\t_this.Show();\n\t\t};\t\t\n\t\tthis.Button.Visible = this.IsEnable();\t\n\t\t//type이 analysis여서 OlapGrid 바이딩 되면... \t\n      \tif(this.OlapGrid) {\n            this.OlapGrid.OnDataBindEnd = function(s, e){\n                _this.Button.Visible = _this.IsEnable();\t\n                if(_this.USE_INIT_LAYOUT){\n                    //layout 업데이트\n                    _this.UpdateDefaultLayout();\t\n                }\n            };\n        }\n\t};\n\t\n\t/* Layout 저장 정보 추출(Serialize)  */\n\tthis.getLayoutModel = function(){\n\t\tvar controls;\n\t\tvar Type = \"OlapTreeView\";\n\t\tvar GridLayout = null;\n\t\tvar TreeViewLayout = null;\n\t\tvar TreeFilterInfo = null;\n\t\t\n\t\t// OlapGrid\n\t\tif(this.OlapGrid){\n\t\t\tcontrols = [];\n\t\t\tthis.OlapGrid.Serialize(controls);\n\t\t\tGridLayout = controls[0];\n\t\t}\n\n\t\t// TreeView\n\t\tif(this.TreeView){\n\t\t\tcontrols = [];\n\t\t\tthis.TreeView.Serialize(controls);\n\t\t\tTreeViewLayout = controls[0];\n\t\t\tTreeFilterInfo = this.TreeView.GetLOVFilterInfo();\n\t\t}\n\t\t\n\t\t//데이터 소스 serialize\n\t\tvar viewer = Matrix.getViewer();\t\t\n\t\tvar ds = {\"DataSources\" : {\"Datas\":[]}, \"MetaDataSources\" : {}};\n\t\tviewer.DataSourceManager.MakeSaveJSON(ds.DataSources.Datas);\n\t\tviewer.MetaDataSourceManager.Serialize(ds.MetaDataSources);\t\t\n\t\t\n\t\treturn {  \"Version\" : 1\n\t\t\t\t  , \"GridType\": Type\n\t\t\t\t  , \"GridLayout\": GridLayout\n\t\t\t\t  , \"TreeViewLayout\": TreeViewLayout // 추가\n\t\t\t\t  , \"FilterInfo\": TreeFilterInfo // 추가\n\t\t\t\t  //, \"CHART_TOGGLE\" : SCRIPT.chartToggle // 삭제\n\t\t\t\t  , \"DATA\":ds\n\t\t\t\t  //, \"Filters\": Filters // 삭제\n\t\t\t\t};\n\t}\n\t\n\t/**\n\t * Layout 저장 정보 표현(Deserialize)\n\t * @param model \n\t */\n\tthis.setLayoutModel = function(model){\t\t\n\t\tvar result = false;\n\t\tif(model){\n\t\t\tif(model[\"GridType\"] !== \"OlapTreeView\") return result;\n\t\t\t\n\t\t\t//데이터 소스 역직열화\n\t\t\tif(model[\"DATA\"]){\n\t\t\t\tvar ds = model[\"DATA\"];\n\t\t\t\tvar viewer = Matrix.getViewer();\t\t\t\t\t\t\t\t\n\t\t\t\tif (ds.DataSources.Datas.length > 0) {\n\t\t\t\t\tviewer.DataSourceManager.SetDataSourceJSON(ds.DataSources.Datas);\n\t\t\t\t\tviewer.MetaDataSourceManager.Deserialize(ds.MetaDataSources); // object\n\t\t\t\t}\t\t\t\t\n\t\t\t}\n\n\t\t\t// OLAP이면\n\t\t\tif(model[\"GridType\"] === \"OlapTreeView\") {\n\t\t\t\tthis.OlapGrid.ClearFields();\n\t\t\t\tthis.OlapGrid.Deserialize(model[\"GridLayout\"]);\n\t\t\t\t// this.OlapGrid.DoRefresh = true;\n\n\t\t\t\t// this.TreeView.ClearDataSet();\n\t\t\t\t// this.TreeView.Deserialize(model[\"TreeViewLayout\"]);\n\t\t\t\tvar this_ = this;\n\t\t\t\tvar aud = SCRIPT._AUD_;\n\t\t\t\tvar filterInfoArr = (model[\"FilterInfo\"] && model[\"FilterInfo\"].length > 0) ? model[\"FilterInfo\"] : []; // Parenthesis Item의 경우 MetaItemCode가 없다\n\t\t\t\tvar filterInfoForUpdate = {}; // Olap 에 반영될 Filter 정보\n\t\t\t\tvar filterInfoForTemp = {}; // 임시로 저장될 Filter 정보\n\t\t\t\tvar parenthesisItems = this.getParenthesisItem(filterInfoArr);\n\t\t\t\tvar currentFilterInfo = this.TreeView.GetLOVFilterInfo();\n\t\t\t\tif(currentFilterInfo && currentFilterInfo.length > 0) {\n\t\t\t\t\tcurrentFilterInfo.forEach(function(info) {\n\t\t\t\t\t\tthis_.TreeView.model.KeyTreeNode[info.MetaItemCode].DataRow[\"HasFilter\"] = false;\n\t\t\t\t\t\tfilterInfoForUpdate[info.MetaItemCode] = {IsDelete: true};\n\t\t\t\t\t})\n\t\t\t\t}\n\n\t\t\t\tif(aud.MetaWizardManager && aud.MetaWizardManager.dialogWindow){\n\t\t\t\t\taud.MetaWizardManager.dialogWindow.AUD.MetaViewManager.Filter.ClearFilterItems();\n\t\t\t\t}\n\n\t\t\t\tfilterInfoArr.forEach(function(info){\n\t\t\t\t\tif(info.Applied) {\n\t\t\t\t\t\tif(typeof info.MetaItemCode === \"string\" && info.MetaItemCode !== \"\") {\n\t\t\t\t\t\t\tthis_.TreeView.model.KeyTreeNode[info.MetaItemCode].DataRow[\"HasFilter\"] = true;\n\t\t\t\t\t\t\tthis_.createFilterObject(filterInfoForUpdate, info, parenthesisItems);\n\t\t\t\t\t\t}\n\t\t\t\t\t} else {\n\t\t\t\t\t\tthis_.TreeView.model.KeyTreeNode[info.MetaItemCode].DataRow[\"HasFilter\"] = true;\n\t\t\t\t\t\tthis_.createFilterObject(filterInfoForTemp, info, parenthesisItems);\n\t\t\t\t\t}\n\t\t\t\t});\n\n\t\t\t\tthis.TreeView.FilterItemsToBeAddedToOlap = filterInfoForUpdate;\n\t\t\t\tif(Object.keys(filterInfoForUpdate).length > 0) {\n\t\t\t\t\taud.MetaViewManager.ActiveMetaTreeView = this.TreeView;\n\t\t\t\t\tthis.TreeView.UpdateFilterItem(false);\n\t\t\t\t\tif (SCRIPT.OnMetaViewerBindClosed) SCRIPT.OnMetaViewerBindClosed = undefined;\n\t\t\t\t\tSCRIPT.OnMetaViewerBindClosed = function () {\n\t\t\t\t\t\tthis_.setCheckForTreeView();\n\t\t\t\t\t};\n\t\t\t\t} else {\n\t\t\t\t\tthis.setCheckForTreeView();\n\t\t\t\t}\n\n\t\t\t\tresult = true;\n\t\t\t}\n\t\t\t\n\t   }\n\n\t   return result;\n\t}\n\t\n\t/** TreeView에 Check여부를 반영하는 메서드 */\n\tthis.setCheckForTreeView = function() {\n\t\tvar aud = SCRIPT._AUD_;\n\t\tvar metaViewGridBindInfoObj = aud.MetaViewManager.MetaViewGridBindInfo.get(this.TreeView.BindGridName);\n\t\tif(metaViewGridBindInfoObj && metaViewGridBindInfoObj.TargetGrid) {\n\t\t\tvar modelMaps = metaViewGridBindInfoObj.TargetGrid.iOLAPView.Model.Fields.getValues();\n\t\t\tvar tempTargetGridModelMap = {};\n\t\t\tif(modelMaps) {\n\t\t\t\tvar keyMaps = Object.keys(modelMaps);\n\t\t\t\tvar obj;\n\t\t\t\tkeyMaps.forEach(function(key, idx){\n\t\t\t\t\tobj = modelMaps[key];\n\t\t\t\t\ttempTargetGridModelMap[obj.MetaItemCode] = obj;\n\t\t\t\t});\n\t\t\t}\n\n\t\t\tif (metaViewGridBindInfoObj.MetaTreeView.TotalRows && tempTargetGridModelMap) {\n\t\t\t\tmetaViewGridBindInfoObj.MetaTreeView.TotalRows.forEach(function (node) {\n\t\t\t\t\tif (tempTargetGridModelMap[node.Key]) {\n\t\t\t\t\t\tnode.IsChecked = true;\n\t\t\t\t\t} else {\n\t\t\t\t\t\tnode.IsChecked = false;\n\t\t\t\t\t}\n\t\t\t\t\tmetaViewGridBindInfoObj.MetaTreeView.CheckedItemsForAfterCalculate[node.Key] = node.IsChecked\n\t\t\t\t});\n\t\t\t}\t\n\t\t\tmetaViewGridBindInfoObj.MetaTreeView.Update();\n\t\t\tmetaViewGridBindInfoObj.TargetGrid.Refresh();\n\t\t\t\n\t\t\t// Olap에 반영안된 것들만 추가 -> 체크박스도 임시저장되는 항목없도록 정해서 필터도 통일성 고려하여 하지 않는 것으로 정함\n\t\t\tthis.TreeView.FilterItemsToBeAddedToOlap = {};\n\t\t\t// this_.TreeView.FilterItemsToBeAddedToOlap = filterInfoForTemp;\n\t\t}\n\t}\n\n\t/**\n\t * AND OR MetaFilterItem을 생성하기 위한 메서드\n\t * @param {*} filterInfoArr \n\t * @returns \n\t */\n\tthis.getParenthesisItem = function(filterInfoArr) {\n\t\tvar resultObj = {};\n\t\tfilterInfoArr.forEach(function(info){\n\t\t\tif(typeof info.MetaItemCode === \"string\" && info.MetaItemCode === \"\") {\n\t\t\t\tresultObj[info.ID] = {Operator : info.Operator};\n\t\t\t}\n\t\t})\n\t\treturn resultObj;\n\t}\n\n\t/**\n\t * 서버에서 받은 FilerInfo를 대상 Object에 배분해주는 메서드\n\t * @param {*} targetObj \n\t * @param {*} filterInfo \n\t * @param {*} parenthesisItems\n\t */\n\tthis.createFilterObject = function(targetObj, filterInfo, parenthesisItems) {\n\t\tif(filterInfo.Values && filterInfo.Values.length > 0) {\n\t\t\tvar values = [];\n\t\t\tvar value;\n\t\t\tfilterInfo.Values.forEach(function(infoValue) {\n\t\t\t\tvalue = {Value: infoValue, Operator: filterInfo.Operator};\n\t\t\t\tif(filterInfo.ParentCode && parenthesisItems[filterInfo.ParentCode]) {\n\t\t\t\t\tvalue.IsAnd = (parenthesisItems[infoValue.ParentCode] === '13'); // enMetaConditionOperator.opAnd\n\t\t\t\t}\n\t\t\t\tvalues.push(value);\n\t\t\t})\n\t\t}\n\n\t\tif(!targetObj[filterInfo.MetaItemCode]) {\n\t\t\ttargetObj[filterInfo.MetaItemCode] = {\n\t\t\t\tIsDelete: false,\n\t\t\t\tCaption: filterInfo.NAME,\n\t\t\t\tLOVResult: values\n\t\t\t};\n\t\t} else {\n\t\t\tif(targetObj[filterInfo.MetaItemCode].IsDelete) {\n\t\t\t\ttargetObj[filterInfo.MetaItemCode] = {\n\t\t\t\t\tIsDelete: false,\n\t\t\t\t\tCaption: filterInfo.NAME,\n\t\t\t\t\tLOVResult: values\n\t\t\t\t};\n\t\t\t} else {\n\t\t\t\ttargetObj[filterInfo.MetaItemCode].LOVResult.push(values[0]);\n\t\t\t}\n\t\t}\n\n\t}\n\n\t/**\n\t * 개인화 보고서 실행\n\t * @returns \n\t */\n\tthis.Show = function(){\n\t\tvar _this = this; \n\t\tif(!this.IsEnable()){\n\t\t\t//Matrix.Information(\"해당 기능은 보고서 저장 후 사용 가능 합니다.\" ,\"저장 확인\");\n\t\t\treturn;\n\t\t}\n\n\t\tvar param = {\n\t\t\t\t\t\t\"OBJECT_ID\" : this.OlapGrid.Name,\n\t\t\t\t\t\t\"REPORT_CODE\" : Matrix.GetReportInfo().CODE,\n\t\t\t\t\t\t\"OBJECT_VALUE\" : this.getLayoutModel()\n\t\t\t\t\t};\n\t\t\t\t\t\n\t\tvar option = {\n                         Width : 450,\t   //너비\n                         Height : 350,\t   //높이\n                         MinWidth : 450,\t  //최소 너비\n                         MinHeight : 350,  //최소 높이\n                         Left : 30,\t     //창의 위치(Left)\n                         Top : 30,\t     //창의 위치(Top)\n                         Center : true,\t //창의 위치를 화면의 가운데로 배치할 지 여부\n                         IsModal  : true,\t //모달창 여부\n                         Title : Matrix.Trans(SCRIPT.langNS + \"TEMPLATE.LAYOUT_TITLE\", \"Setting User Defined Layout\"),\t//팝업창의 타이틀\n                         Maximize : false,\t//최대화 버튼 활성화 여부\n                         Resizable : false,\t//창 사이즈 조정 기능 활성화 여부\n                         Buttons : 0\t       // 버튼 타입 ( 0 : 없음, 1:닫기, 2:확인/취소)\n        };\t\t\n\t\tMatrix.ShowReportDialog(this.LayoutReportName, param, option, function(result) {\n\t\t\t\tif(result){\n\t\t\t\t\t_this.setLayoutModel(result);\n\t\t\t\t\tif(result[\"USE_DEFAULT_LAYOUT\"] === true){\n\t\t\t\t\t\t_this.USE_INIT_LAYOUT = true;\n\t\t\t\t\t}else{\n\t\t\t\t\t\t_this.USE_INIT_LAYOUT = false;\n\t\t\t\t\t}\n\t\t\t\t\t//Matrix.doRefresh(_this.OlapGrid);\n\t\t\t\t}\n\t\t\t});\n\t};\n\t\n\t/**\n\t * 개인화 초기 실행\n\t * @param callBack \n\t * @returns \n\t */\n\tthis.CheckDefaultLayout = function(callBack){\n\t\tvar _this = this;\n\t\tif(!this.IsEnable()){\n\t\t\tif(callBack){callBack();}\n\t\t\treturn;\n\t\t}\n\t\tMatrix.AddGlobalParams(\"VS_TARGET_REPORT_CODE\" ,Matrix.GetReportInfo().CODE, 1);  /*String */\n\t\tMatrix.AddGlobalParams(\"VS_TARGET_OBJECT_ID\" ,\"*\", 1);  /*String */\t\t\n\t\tMatrix.RunScript(\"\" ,\"@\" + this.LayoutReportName + \"@INIT_LAYOUT\" ,function(p){\n                                Matrix.RemoveGlobalParams(\"VS_TARGET_REPORT_CODE\");\n                                Matrix.RemoveGlobalParams(\"VS_TARGET_OBJECT_ID\");\n\t\t\t\t\t\t\t\tif(p.Success == false){\n                               \t\t//Matrix.Alert(p.Message);\n\t\t\t\t\t\t\t\t\tif(callBack){callBack();} //callback\n                               \t\treturn;\n                               \t}\n                               \tvar ds = p.DataSet;\n\t\t\t\t\t\t\t\tvar jsonObj = null;\n\t\t\t\t\t\t\t\tif(ds.GetTable(0).GetRowCount() > 0){\n\t\t\t\t\t\t\t\t\tvar json = ds.GetTable(0).getData(0 ,\"OPTION_VALUE\");\n\t\t\t\t\t\t\t\t\tif(json){\n\t\t\t\t\t\t\t\t\t\tjsonObj = JSON.parse(json);\n\t\t\t\t\t\t\t\t\t   if(_this.setLayoutModel(jsonObj)) _this.USE_INIT_LAYOUT = true; //사용함.\n\t\t\t\t\t\t\t\t\t   // Matrix.doRefresh( _this.OlapGrid.Name);\n\t\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\t}\n\t\t\t\t\t\t\t\tif(callBack){callBack();} //callback\n\t\t\t\t\t\t\t\t\n                               });\t\t\n\t\t\n\t\t\n\t};\n\t\n\t/**\n\t * 조건 개인화 업데이트\n\t * @returns \n\t */\n\tthis.UpdateDefaultLayout = function(){\n\t\ttry{\n\t\t\tif(!this.IsEnable()){ return; }\n\t\t\t\n\t\t\tMatrix.SetUseProgressBar(false);\n\t\t\t\n\t\t\tMatrix.AddGlobalParams(\"VS_TARGET_REPORT_CODE\" ,Matrix.GetReportInfo().CODE, 1);  /*String */\n\t\t\tMatrix.AddGlobalParams(\"VS_TARGET_OBJECT_ID\" ,\"*\", 1);  /*String */\t\t\n\t\t\t\n\t\t\tvar objectValue = this.getLayoutModel();\n\t\t\tMatrix.AddGlobalParams(\"VS_TARGET_OBJECT_VALUE\", JSON.stringify(objectValue) ,1);\n\t\t\t\n\t\t\tMatrix.RunScript(\"\" ,\"@\" + this.LayoutReportName + \"@UPDATE_INIT_LAYOUT\" ,function(p){\n\t\t\t\t\t\t\t\t\tMatrix.RemoveGlobalParams(\"VS_TARGET_REPORT_CODE\");\n\t\t\t\t\t\t\t\t\tMatrix.RemoveGlobalParams(\"VS_TARGET_OBJECT_ID\");\n\t\t\t\t\t\t\t\t\tMatrix.RemoveGlobalParams(\"VS_TARGET_OBJECT_VALUE\");\t\t\t\t\t\t\t\t\t\n\t\t\t\t\t\t\t\t\tMatrix.SetUseProgressBar(true);\n                               });\t\n\t\t\t\t\t\t\t   \n\t\t\t \n\t\t}catch(e){}\n\t\t\n\t\tMatrix.SetUseProgressBar(true);\n\t\t\n\t};\n\t\n\tthis.init();\n}\n\nSCRIPT.USERDEFINED_LAYOUT_MGR = new UserDefinedLayoutManager(arguments[0], arguments[1], arguments[2], arguments[3]);",
