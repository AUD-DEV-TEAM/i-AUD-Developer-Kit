  "ScriptText": "/* Init Controls */\nvar GRP_BODY = Matrix.getObject(\"GRP_BODY\");\nvar GRD_CUSTOMER = Matrix.getObject(\"GRD_CUSTOMER\");\nvar VS_KEYWORD = Matrix.getObject(\"VS_KEYWORD\");\nvar LBL_TTL_3 = Matrix.getObject(\"LBL_TTL_3\");\n/* Button Controls */\nvar BTN_REF = Matrix.getObject('BTN_REF');\nvar BTN_DEL = Matrix.getObject('BTN_DEL');\nvar BTN_RESET = Matrix.getObject('BTN_RESET');\nvar BTN_SAV = Matrix.getObject('BTN_SAV');\nvar BTN_ADD = Matrix.getObject('BTN_ADD');\n/* Input Controls */\nvar VS_INP_NAME = Matrix.getObject(\"VS_INP_NAME\");\nvar VS_INP_TYPE = Matrix.getObject(\"VS_INP_TYPE\");\nvar VS_INP_GRADE = Matrix.getObject(\"VS_INP_GRADE\");\nvar VS_INP_REG_NO = Matrix.getObject(\"VS_INP_REG_NO\");\nvar VS_INP_MAIN = Matrix.getObject(\"VS_INP_MAIN\");\nvar VS_INP_PHONE = Matrix.getObject(\"VS_INP_PHONE\");\nvar VS_INP_EMAIL = Matrix.getObject(\"VS_INP_EMAIL\");\nvar VN_INP_LIMIT = Matrix.getObject(\"VN_INP_LIMIT\");\nMatrix.OnDocumentLoadComplete = function (s, e) {\n    VS_KEYWORD.UsePlaceholder = true;\n    VS_KEYWORD.SetPlaceholder(' 고객명, 사업자번호로 검색');\n};\nMatrix.OnLoadComplete = function (s, e) {\n    if (e.Message) {\n        initInputValue();\n    }\n};\nMatrix.OnDataBindEnd = function (s, e) {\n    if (e.Id == 'GRD_CUSTOMER') {\n        Matrix.getObject('LBL_TTL_2').Text = '   고객 목록 (' + e.RecordCount + '개사)';\n    }\n    else if (e.Id == 'VS_INP_TYPE') {\n        VS_INP_TYPE.Value = '';\n    }\n    else if (e.Id == 'VS_INP_GRADE') {\n        VS_INP_GRADE.Value = '';\n    }\n};\n/******** Button Click Event ********/\n// 검색\nBTN_REF.OnClick = function (s, e) {\n    Matrix.doRefresh('GRD_CUSTOMER');\n};\n// 삭제\nBTN_DEL.OnClick = function (s, e) {\n    var checkCount = 0;\n    for (var i = 0; i < GRD_CUSTOMER.GetRowCount(); i++) {\n        if (GRD_CUSTOMER.getRowValue(i, 'CHK') == 'Y') {\n            GRD_CUSTOMER.ChangeRowStateAt(i, 'D');\n            checkCount++;\n        }\n    }\n    if (!checkCount) {\n        Matrix.Information('삭제할 항목을 선택하세요', '안내');\n        return;\n    }\n    Matrix.Confirm('선택한 항목을 삭제하시겠습니까?', '안내', function (ok) {\n        if (ok) {\n            Matrix.RunScript('GRD_CUSTOMER', 'GRD_CRUD', function (p) {\n                if (p.Success == false) {\n                    Matrix.Alert(p.Message);\n                    return;\n                }\n                Matrix.doRefresh('GRD_CUSTOMER');\n                Matrix.Information('삭제 완료되었습니다.', '안내');\n            });\n        }\n        else {\n            GRD_CUSTOMER.ClearRowState(false);\n        }\n    }, 0);\n};\n// 초기화\nBTN_RESET.OnClick = function (s, e) {\n    initInputValue();\n};\n// 저장\nBTN_SAV.OnClick = function (s, e) {\n    var savFields = [VS_INP_NAME.Text, VS_INP_TYPE.Value, VS_INP_GRADE.Value];\n    var savControls = [VS_INP_NAME, VS_INP_TYPE, VS_INP_GRADE];\n    var savInvalid = isInvalidInput(savFields, savControls);\n    if (savInvalid) {\n        Matrix.Information('필수 입력 항목을 확인해주세요', '안내');\n        savInvalid.Focus();\n        return;\n    }\n    if (LBL_TTL_3.Text.includes('등록')) {\n        Matrix.RunScript('', 'GRD_INSERT', function (p) {\n            if (p.Success == false) {\n                Matrix.Alert(p.Message);\n                return;\n            }\n            Matrix.doRefresh('GRD_CUSTOMER');\n            Matrix.Information('추가 완료되었습니다.', '안내');\n        });\n    }\n    else if (LBL_TTL_3.Text.includes('수정')) {\n        Matrix.RunScript('GRD_CUSTOMER', 'GRD_CRUD', function (p) {\n            if (p.Success == false) {\n                Matrix.Alert(p.Message);\n                return;\n            }\n            Matrix.doRefresh('GRD_CUSTOMER');\n            Matrix.Information('수정 완료되었습니다.', '안내');\n        });\n    }\n};\n// 추가\nBTN_ADD.OnClick = function (s, e) {\n    GRD_CUSTOMER.ClearSelect();\n    Matrix.SetGlobalParams('VS_CUST_ID', '');\n    initInputValue();\n    LBL_TTL_3.Text = '   고객 신규 등록';\n};\n/******** Button Click Event ********/\nVS_KEYWORD.OnTextKeydown = function (s, e) {\n    if (e.Event.isEnter()) {\n        Matrix.doRefresh('GRD_CUSTOMER');\n    }\n};\nGRD_CUSTOMER.OnDataBindEnd = function (s, e) {\n    GRD_CUSTOMER.ClearSelect();\n    Matrix.SetGlobalParams('VS_CUST_ID', '');\n    initInputValue();\n    LBL_TTL_3.Text = '   고객 신규 등록';\n};\nGRD_CUSTOMER.OnGridMultiHeaderCheckBoxClicked = function (s, e) {\n    var checkValue = e.Checked ? 'Y' : 'N';\n    for (var i = 0; i < GRD_CUSTOMER.GetRowCount(); i++) {\n        GRD_CUSTOMER.setRowValue(i, 'CHK', checkValue);\n    }\n    GRD_CUSTOMER.Update();\n};\nMatrix.OnViewerSizeChanged = function (s, e) {\n    GRP_BODY.Width = e.Width - 520;\n};\nGRD_CUSTOMER.OnCellClick = function (s, e) {\n    GRD_CUSTOMER.ClearRowState(false);\n    GRD_CUSTOMER.ChangeRowState(e.Row, 'U');\n    GRD_CUSTOMER.ReDraw();\n    Matrix.SetGlobalParams('VS_CUST_ID', e.Row.GetValue('CUST_ID'));\n    LBL_TTL_3.Text = '   고객 정보 수정';\n};\nvar initInputValue = function () {\n    VS_INP_NAME.Text = '';\n    VS_INP_TYPE.Value = '';\n    VS_INP_GRADE.Value = '';\n    VN_INP_LIMIT.Value = 0;\n    VS_INP_REG_NO.Text = '';\n    VS_INP_PHONE.Text = '';\n    VS_INP_EMAIL.Text = '';\n    VS_INP_MAIN.Text = '';\n};\nvar isInvalidInput = function (fields, controls) {\n    var idx = fields.findIndex(function (v) {\n        return v === null || v === undefined || v === '';\n    });\n    if (idx !== -1 && controls && controls[idx]) {\n        return controls[idx];\n    }\n    return idx !== -1 ? true : null;\n};\n",
