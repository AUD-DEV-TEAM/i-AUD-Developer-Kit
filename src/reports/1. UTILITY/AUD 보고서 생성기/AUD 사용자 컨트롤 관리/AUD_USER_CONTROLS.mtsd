  "WorkFlowModules": [
    {
      "EVENT_YN": "N",
      "CREATE_USER": "yglee",
      "MODULE_DESCRIPTION": "좌/우 컨트롤을 Resize할 수 있는 Splitter 기능을 제공합니다.\n라벨 컨트롤 또는 이미지 등을 Spliter 구분으로 사용하실 수 있습니다.",
      "SCRIPT_TEXT": "/*\n경고! 이 모듈은 시스템 모듈로 시스템 업데이트 시 초기화 될 수 있으니 직접 수정하지 마십시요\nwarning! This module is a system module and may be initialized when the system is updated, so do not modify it directly.\n\nSplitter 좌/우\n*/\t\t\n\nvar ColumnSplitter = /** @class */ (function () {\n    function ColumnSplitter(left, right, splitter) {\n        this.mBeginDrag = false;\n        this.mMouseDownX = -1;\n        this.mSplitterX = -1;\n        this.mBoundLeft = -1;\n        this.mBoundRight = -1;\n        this.MARGIN = 2;\n        var _this = this;\n        this.mSplitter = Matrix.getObject(splitter);\n        this.mSplitter.Cursor = \"e-resize\";\n        this.mSplitter.Text = \" \"; //공백을 주지 않으니, Cursor가 먹지 않는다.\n        this.minLeft = 50;\n        this.minRight = 50;\n        var ctl;\n        this.mLeftControls = new Array();\n        left.forEach(function (name) {\n            ctl = Matrix.getObject(name);\n            if (ctl) {\n                _this.mLeftControls.push(ctl);\n                _this.minLeft = Math.max(_this.minLeft, ctl.Docking.MinWidth);\n                ctl.Width = _this.mSplitter.Left - ctl.Left - _this.MARGIN;\n            }\n        });\n        this.mRightControls = new Array();\n        right.forEach(function (name) {\n            ctl = Matrix.getObject(name);\n            if (ctl) {\n                _this.mRightControls.push(ctl);\n                _this.minRight = Math.max(_this.minRight, ctl.Docking.MinWidth);\n                ctl.Left = _this.mSplitter.Left + _this.mSplitter.Width + _this.MARGIN;\n            }\n        });\n        var OnMouseDown = function (event) {\n            _this.mBeginDrag = true;\n            _this.mMouseDownX = event.clientX;\n            _this.mBoundLeft = event.clientX; //_this.ControlL.Left;\n            _this.mLeftControls.forEach(function (ctl) {\n                _this.mBoundLeft = Math.min(_this.mBoundLeft, ctl.Left);\n            });\n            _this.mSplitterX = _this.mSplitter.Left;\n            //_this.mBoundRight   = _this.ControlR.Left + _this.ControlR.Width;\n            _this.mBoundRight = 0;\n            _this.mRightControls.forEach(function (ctl) {\n                _this.mBoundRight = Math.max(_this.mBoundRight, (ctl.Left + ctl.Width));\n            });\n            event.preventDefault();\n        };\n        var OnMouseMove = function (event) {\n            if (_this.mBeginDrag) {\n                var diff = Math.abs(event.clientX - _this.mMouseDownX);\n                var splitLeft = 0;\n                if (event.clientX > _this.mMouseDownX) { //우측으로 이동\n                    splitLeft = _this.mSplitterX + diff; // splitPosition\n                }\n                else { //좌측으로 이동\n                    splitLeft = _this.mSplitterX - diff; // splitPosition\n                }\n                if (splitLeft - _this.mBoundLeft > _this.minLeft //check min left\n                    && _this.mBoundRight - splitLeft - _this.mSplitter.Width >= _this.minRight) {\n                    _this.mSplitter.Left = splitLeft;\n                    //_this.ControlL.Width  = splitLeft - _this.mBoundLeft - _this.MARGIN;\n                    _this.mLeftControls.forEach(function (ctl) {\n                        ctl.Width = splitLeft - ctl.Left - _this.MARGIN;\n                    });\n                    _this.mRightControls.forEach(function (ctl) {\n                        var rect = { \"Left\": 100000, \"Top\": 100000, \"Width\": 0, \"Height\": 0, \"Right\": 0, \"Bottom\": 0 };\n                        rect.Top = ctl.Top;\n                        rect.Height = ctl.Height;\n                        rect.Left = (_this.mSplitter.Left + _this.mSplitter.Width) + _this.MARGIN;\n                        rect.Width = (ctl.Left + ctl.Width) - rect.Left;\n                        ctl.setRect(rect);\n                        ctl.Resize();\n                    });\n                }\n                event.preventDefault();\n            }\n        };\n        var OnMouseUp = function (event) {\n            if (_this.mBeginDrag) {\n                event.preventDefault();\n                _this.mBeginDrag = false;\n            }\n        };\n        $(_this.mSplitter.Element).on(\"mousedown\", OnMouseDown);\n        $(window.document).on(\"mousemove\", OnMouseMove);\n        $(window.document).on(\"mouseup\", OnMouseUp);\n        Matrix.OnDestroy = function (sender, args) {\n            //이벤트 해제 하기\n            $(_this.mSplitter.Element).off(\"mousedown\", OnMouseDown);\n            $(window.document).off(\"mousemove\", OnMouseMove);\n            $(window.document).off(\"mouseup\", OnMouseUp);\n        };\n    }\n    return ColumnSplitter;\n}());\n\nnew ColumnSplitter(arguments[0].split(\",\"), arguments[1].split(\",\"),arguments[2]);\n",
